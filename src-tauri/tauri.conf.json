{
    "build": {
        "beforeBuildCommand": "npm run build",
        "beforeDevCommand": "npm run dev",
        "devPath": "http://localhost:3000",
        "distDir": "../build"
    },
    "package": {
        "productName": "POS_Report_Helper",
        "version": "0.3.17"
    },
    "tauri": {
        "allowlist": {
            "window": {
                "all": true
            },
            "shell": {
                "sidecar": true,
                "scope": [
                    {
                        "name": "bin/python/server",
                        "sidecar": true
                    }
                ]
            }
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [
                "bin/python/server"
            ],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "exports.alexalder.dev",
            "longDescription": "",
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "fullscreen": false,
                "center": true,
                "height": 1000,
                "resizable": false,
                "title": "POS_Report_Helper 0.310.2023",
                "width": 1500
            }
        ]
    }
}